{{#with site.keys.cookiebot}}
    <script id="Cookiebot" src="https://consent.cookiebot.com/uc.js" data-cbid="{{this}}" data-blockingmode="auto" type="text/javascript"></script>
{{/with}}
{{#with site.keys.googleAnalytics}}
    <script async src="https://www.googletagmanager.com/gtag/js?id={{this}}"></script>
    <script>function gtag(){dataLayer.push(arguments)};window.dataLayer=window.dataLayer||[];gtag('js',new Date());gtag('config','{{this}}')</script>
{{/with}}

{{!-- Override default Antora search with Meilisearch --}}
<script>
// Override Antora search initialization to prevent conflicts
window.antoraSearch = { initSearch: function() { console.log('Antora search disabled, using Meilisearch'); } };
</script>
<script src="{{{uiRootPath}}}/js/vendor/meilisearch.umd.js"></script>
<script src="{{{uiRootPath}}}/js/search-config.js"></script>
<script src="{{{uiRootPath}}}/js/vendor/meilisearch-search.js"></script>

{{#with site.keys.segmentWrite }}
    <script src="https://cdn.jsdelivr.net/npm/js-cookie@3.0.5/dist/js.cookie.min.js"></script>
{{/with}}

